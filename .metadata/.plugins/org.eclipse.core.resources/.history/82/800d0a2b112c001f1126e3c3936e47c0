package in.shiv.controller;

import org.springframework.web.bind.annotation.RestController;

import in.shiv.binding.Book;

@RestController
public class BookRestController {
	
	public Book getBooks() {
		
		Book book = new Book();
		book.setId(101);
		book.setName("SpringBoot");
		book.setPrice(400.0);
		return book;
	}

}
